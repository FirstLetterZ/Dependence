<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DashedView">
        <attr name="dashColor" format="color"/>
        <attr name="dashGap" format="dimension"/>
        <attr name="dashWidth" format="dimension"/>
        <attr name="strokeWidth" format="dimension"/>
        <attr name="dashType" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
            <enum name="circle" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="TriangleView">
        <attr name="triangle_color" format="color" />
        <attr name="triangle_direction" format="enum">
            <enum name="left" value="0" />
            <enum name="top" value="1" />
            <enum name="right" value="2" />
            <enum name="bottom" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="TagTextView">
        <attr name="ellipsisColor" format="color" />
        <attr name="ellipsisText" format="string"/>
        <attr name="maxLines" format="integer"/>
        <attr name="paragraphSpace" format="dimension"/>
        <attr name="lineHeight" format="dimension"/>
        <attr name="maxHeight" format="dimension"/>
        <attr name="fontSize" format="dimension"/>
        <attr name="fontColor" format="color"/>
        <attr name="bold" format="boolean"/>
        <attr name="italic" format="boolean"/>
        <attr name="underline" format="boolean"/>
        <attr name="strikeThru" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="PhonePageLayout">
        <attr name="isContentView" format="boolean"/>
        <attr name="hierarchy" format="integer" />
        <attr name="contentBelowTitle" format="boolean" />
    </declare-styleable>
</resources>